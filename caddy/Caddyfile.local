:8080 {

  encode gzip

  handle /api/* {
    reverse_proxy backend:8000
  }

  handle /static/* {
    reverse_proxy backend:8000
  }

  handle /login* {
    reverse_proxy backend:8000
  }

  handle /logout* {
    reverse_proxy backend:8000
  }

  handle /app* {
    reverse_proxy backend:8000
  }

  handle {
    reverse_proxy landing:3000
  }
}
